{
  "name": "vemetric/vemetric-php",
  "description": "Official PHP SDK for Vemetric",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Vemetric\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vemetric\\Tests\\": "tests/"
    }
  },
  "keywords": [
    "vemetric",
    "analytics",
    "sdk"
  ],
  "homepage": "https://vemetric.com/docs/sdks/php",
  "support": {
    "issues": "https://github.com/Vemetric/vemetric-php/issues",
    "source": "https://github.com/Vemetric/vemetric-php"
  }
}
